{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "development"
    },
    "staging": {
      "distribution": "internal",
      "channel": "staging",
      "env": {
        "APP_ENV": "staging"
      }
    },
    "beta": {
      "distribution": "store",
      "channel": "beta",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "beta",
        "EXPO_PUBLIC_SUPABASE_URL": "https://beta-project-ref.supabase.co",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "beta-anon-key-placeholder"
      },
      "ios": {
        "buildConfiguration": "Release",
        "bundleIdentifier": "com.thinkmodalabs.pbr-mvp.beta"
      },
      "android": {
        "buildType": "aab",
        "package": "com.thinkmodalabs.pbr_mvp.beta"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "beta": {
      "ios": {
        "appleId": "rahwalton9@gmail.com",
        "ascAppId": "6753880318",
        "appleTeamId": "6W36XJQB7N"
      },
      "android": {
        "track": "internal",
        "serviceAccountKeyPath": "./secrets/playstore-service-account.json"
      }
    },
    "production": {
      "ios": {
        "appleId": "rahwalton9@gmail.com",
        "ascAppId": "6753880318",
        "appleTeamId": "6W36XJQB7N"
      },
      "android": {
        "serviceAccountKeyPath": "./secrets/playstore-service-account.json"
      }
    }
  }
}
